" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container align-center\">\n        <div class=\"row\">\n            <div class=\"col-md-12 col-lg-9 d-flex flex-column\">\n                <h1 class=\"mbr-section-title mbr-regular pb-3 align-left mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\"><br>ДРУГ — ПОДАРОК</h1>\n                <h3 class=\"mbr-section-subtitle align-left align-left mbr-regular pb-3 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showSubTitle\">Lorem ipsum dolor sit amet, consectetur adipiscing\n                    elit.</h3>\n                <p class=\"mbr-text pb-3 align-left mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">ГИПОАЛЛЕРГЕННЫЕ ДРУЖЕЛЮБНЫЕ ИГРУШКИ-АМИГУРУШКИ</p>\n                <div mbr-buttons mbr-theme-style=\"display-7\" class=\"align-left mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-md btn-primary\" href=\"https://www.livemaster.ru/rumi-rumi\" data-app-placeholder=\"Введите Текст\">КУПИТЬ</a></div>\n            </div>\n            <div class=\"col-lg-3 col-md-12 align-center\">\n                <div class=\"wrap\">\n                    <img src=\"@PROJECT_PATH@/assets/images/kukly-i-igrushki-vyazanyj-zajchik-368x491.jpeg\" alt class=\"image-1 animated-elements1\" title>\n                    <img src=\"@PROJECT_PATH@/assets/images/kukly-i-igrushki-vyazanyj-myshonok-370x493.jpeg\" alt class=\"image-2 animated-elements2\" title>\n                    <img src=\"@PROJECT_PATH@/assets/images/b72dfa9ea62367e7621ab654bb